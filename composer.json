{
    "name": "aboutcoders/supervisor-command-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle that provides console commands to control supervisor instances on top of the https://github.com/yzalis/SupervisorBundle.",
    "keywords": ["supervisor", "supervisord", "command", "symfony"],
    "homepage": "https://bitbucket.org/hasc/supervisor-command-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "schulz@daten-bahn.de"
        }
    ],
    "require": {
        "symfony/symfony": "~2.7|~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "yzalis/supervisor-bundle": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "symfony/monolog-bundle": "~2"
    },
    "autoload": {
        "psr-4": { "Abc\\Bundle\\SupervisorCommandBundle\\": "" }
    }
}
